
public class Main{

    public static double harmonicAvarage(int[] series) {
        int n = series.length;
        double harmonicSeries = 0;
        for (int i = 0; i < n; i++) {
            harmonicSeries += 1.0 / series[i];
        }
        return n / harmonicSeries;
    }

    public static void main(String[] args) {
        
        int[] series = {2, 4, 6, 8, 10};
        System.out.println("Harmonic avarage of series: " + harmonicAvarage(series));
    }
}